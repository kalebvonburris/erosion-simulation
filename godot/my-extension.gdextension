[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust/target/release/liberosion_simulation_lib.so"
linux.release.x86_64 =   "res://../rust/target/release/liberosion_simulation_lib.so"
windows.debug.x86_64 =   "res://../rust/target/debug/liberosion_simulation_lib.dll"
windows.release.x86_64 = "res://../rust/target/release/liberosion_simulation_lib.dll"
macos.debug =            "res://../rust/target/debug/liberosion_simulation_lib.dylib"
macos.release =          "res://../rust/target/release/liberosion_simulation_lib.dylib"
macos.debug.arm64 =      "res://../rust/target/debug/liberosion_simulation_lib.dylib"
macos.release.arm64 =    "res://../rust/target/release/liberosion_simulation_lib.dylib"
web.debug.wasm32 =       "res://../rust/target/wasm32-unknown-emscripten/debug/erosion_simulation_lib.wasm"
web.release.wasm32 =     "res://../rust/target/wasm32-unknown-emscripten/release/erosion_simulation_lib.wasm"